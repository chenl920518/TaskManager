/* 璐ㄦ鎶ュ憡 */
.quality{
	padding:0 10px;
	padding-top:1.5rem;
}
.quality .title{
	width:100%;
	position: fixed;
	left:0;
	top:0;
	height:1.5rem;
	background-color:#ffc0cb;
	font-size:0.75rem;
	text-align:center;
	line-height:1.5rem;
    color: #1B6D85;
}
.quality .wrap p{
	border-top:2px solid #0db1ff;
	color:#000;
	margin-top:0.375rem;
}
.quality .title span{
	margin:0.3rem 0 0 10px;
	float:left;
}
.quality .wrap1 p{
	font-size:0.625rem;
}
.quality .wrap1 ul{
	font-size:0.6rem;
}
.quality .wrap1 li{
	margin-bottom:0.25rem;
}
.quality .wrap1 input{
	font-size:0.5rem;
	margin-left:0.625rem;
	width:6.75rem;
}
.quality .wrap1 button{
	width: 1.75rem;
    height: 0.875rem;
    border-radius: 4px;
    font-size: 0.5rem;
    background-color:#0db1ff;
    color:#fff;
}
.quality .wrap1 span{
	display:inline-block;
	width:6rem;
	text-align: right;
}
.quality .wrap2 p{
	font-size: 0.625rem;
}
.quality .wrap2 ul{
	width:18.925rem;
}
.quality .wrap2 li{
	border:1px solid #ccc;
	width:4.475rem;
	height:3.375rem;
	margin-right:0.25rem;
	margin-bottom:0.25rem;
	float:left;
	position: relative;	
}
.quality .wrap2 li .img_in{
	width:4.475rem;
	height:3.375rem;
	text-align:center;
	line-height:3.375rem;
}
.quality .wrap2 li .img_in img{
	max-width:4.475rem;
	max-height:3.375rem;
	width:auto;
	height:auto;
}
.quality .wrap2 li span{
	position: absolute;
	top:0.25rem;
	right:0.25rem;
	color:#999;
	font-size:12px;
}
.quality .b_z{
	margin-bottom:10px;
}
@media screen and (max-width:768px){
	.quality .wrap2 ul{
		width:18.925rem;
	}
	.quality .wrap2 li{
		border:1px solid #ccc;
		width:22.8%;
		height:3.375rem;
		margin-right:0.25rem;
		margin-bottom:0.25rem;
		float:left;
		position: relative;	
		text-align: center;
		line-height:3.375rem;

	}
	.quality .wrap2 li .img_in{
		width:22.8%;
		height:3.375rem;
		text-align:center;
		line-height:3.375rem;
	}
	
}
