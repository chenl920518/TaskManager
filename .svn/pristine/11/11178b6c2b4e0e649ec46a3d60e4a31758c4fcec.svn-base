.quality .wrap2 li em{
	font-size:1rem;
	color:#ccc;
}
.quality .wrap2 li:last-child{
	position:relaitve;
}
.quality .wrap2 li input{
	position:absolute;
	top:0;
	left:0;
	opacity:0;
	width:65px;
}
.quality .wrap2 .sele_,.quality .wrap2 .report{
	font-size:0.6rem;
}
.quality .wrap2 .sele_ label{
	font-weight:normal;
}
.quality .wrap2 .sele_ select{
	width: 5.25rem;
	height:1.125rem;
}
.quality .report input{
	float:left;
}
.quality .report em{
	float:left;
	margin-right:1.25rem;
}
.quality .btns{
	margin-top:0.375rem;
}
.quality .btns button{
	background-color: #0db1ff;
	color:#fff;
	font-size: 0.5rem;   
}
.quality textarea{
	font-size:0.5rem;
}
input::-webkit-input-placeholder, textarea::-webkit-input-placeholder {
  color: red;
}
input::-moz-placeholder, textarea::-moz-placeholder {
  color: red;
}
input:-ms-input-placeholder, textarea:-ms-input-placeholder {
  color: red;
}
@media screen and (max-width:767px){
	.quality .report input{
		margin-top:2px;
	}
}
@media screen and (min-width:767px){
	.quality .report input{
		margin-top:8px;
	}
}
.quality .wrap2 .report .task{
	display:none;
}
