.quality_report{
	/* padding-left:0;
	padding-right:0; */
}
.quality_report .wrap_top{
	width:100%;
	position: relative;
	border:1px solid #ddd;
	top:10px;
}
.quality_report .btn{
	position: absolute;
	top:50%;
    font-size: 40px;
    margin-top: -40px;
    color:#fff;
    z-index: 10;        
} 
.quality_report .btn span{
	background-color:rgba(0,0,0,.5);
    text-align:center;	
    padding:5px;
}
.quality_report .btn1{left:0;}
.quality_report .btn2{right:0;}
.quality_report .imgs{
	overflow:hidden;
}
.quality_report .imgs ul{
	width:1250px;
	position:relative;
	left:0;
}
.quality_report .imgs ul li{
	float:left;
}
.quality_report .imgs ul li a{
	display:block;
}
.quality_report .wrap_top .bottom{
	position: absolute;
	left:0;
	bottom:0;
	height:35px;
	background-color:rgba(0,0,0,.5);
	width:100%;
	padding-top:8px;
	padding-left:6px;
	padding-right:6px;
}
.quality_report .bottom button{
	padding-left:6px;
	padding-right:6px;
}
.quality_report .bottom span{
	display: inline-block;
	height:20px;
	background-color:#eee;
	padding-left:6px;
	padding-right:6px;
}
.quality_report .s_m{
	width:100%;
}
.quality_report .s_m_1{
	margin-top:20px;
}
.quality_report h3{
	margin-top:10px;
}
.quality_report .s_m p{
	width:100%;
	border:1px solid #ddd;
	margin-top:10px;
	min-height:50px;
}
.quality_report .imgs{
	text-align:center;
}
.quality_report .imgs img{
	width:auto;
	height:auto;
}
.quality_report .div_a{
	margin-top:6px;
}
